{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:remix && npm run build:deno",
    "build:remix": "remix build",
    "build:deno": "esbuild --define:process.env.NODE_ENV='\"production\"' --format=esm --minify --bundle --sourcemap --outdir=dist ./deno/entry.ts",
    "dev": "remix dev",
    "postinstall": "patch-package && remix setup cloudflare-workers",
    "start": "deno run --allow-net --allow-read ./dist/entry.js"
  },
  "dependencies": {
    "@remix-run/cloudflare-workers": "^1.0.6",
    "@remix-run/react": "^1.0.6",
    "mime": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "patch-package": "^6.4.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
